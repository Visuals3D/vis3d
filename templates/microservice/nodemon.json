{
  "watch": ["src"],
  "ext": "ts",
  "ignore": ["src/**/*.spec.ts"],
  "exec": "ts-node ./src/index.ts",
  "events": {
    "start": "npx openapi-typescript swagger.yaml --output  ./models/generated/api/api.schema.ts",
    "restart": "npx openapi-typescript swagger.yaml --output  ./models/generated/api/api.schema.ts"
  }
}
